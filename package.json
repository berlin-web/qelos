{
  "name": "@qelos/monorepo",
  "version": "2.0.21",
  "main": "index.js",
  "scripts": {
    "postinstall": "lerna bootstrap",
    "install:prod-only": "lerna bootstrap -- --production",
    "prebuild": "lerna run prebuild --parallel",
    "build": "npm run prebuild && lerna run build",
    "clean": "lerna clean --yes",
    "dev": "env-cmd lerna run dev --parallel",
    "predev": "kill-port 3000 3001 9000 9001 9002 9003 9004 9005 9006 9007 && docker rm -f qelos-db",
    "postdev": "npm run predev",
    "populate-db": "cd apps/auth && node helpers/init.js",
    "pack-package": "lerna exec npm pack",
    "rename-pack": "lerna exec node ../../tools/bundler/rename-pack.js",
    "test": "lerna run test --parallel"
  },
  "author": "David Meir-Levy <davidmeirlevy@gmail.com>",
  "engines": {
    "node": ">=20.x"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.22",
    "env-cmd": "^10.1.0",
    "jest": "^27.5.1",
    "kill-port": "^2.0.1",
    "lerna": "^6.1.0",
    "ts-node": "^10.2.1",
    "tsx": "^3.12.7"
  }
}
